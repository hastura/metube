	%body
		%nav{:class => ['navbar', 'navbar-default', 'navbar-metube']}
			.container-fluid
				.row
					.col-xs-8.col-md-2#metube-nav-left
						%p{:class => ['navbar-text', 'navbar-left','navbar-metube-wrapper'],}
							%a{:href => "#",:class => ['navbar-link', 'navbar-metube-text','.sidebar-icon']}
								%i.fa.fa-navicon{"aria-hidden" => "true"}
							%a{:href => "#",:class => ['navbar-link', 'navbar-metube-text']}
								%img{:src => "assets/images/metube.png", :alt => ""}/

					.col-xs-4.hidden-md.hidden-lg#metube-nav-right
						%p{:class => ['navbar-text', 'navbar-right','navbar-metube-wrapper'],}
							%a{:class => ['navbar-link', 'navbar-metube-text'],:href => "#"}
								%i.fa.fa-upload{"aria-hidden" => "true"}
								%span.hidden-xs.hidden-sm	Upload
							%a{:class => ['navbar-link', 'navbar-metube-text'],:href => "#"}
								%i.fa.fa-user-circle-o{"aria-hidden" => "true"}
								%span.hidden-xs.hidden-sm Login/Register

					.col-xs-12.col-md-6#search-box
						.input-group
							%input.form-control{:placeholder => "Cari Video"}/
							%span.input-group-btn
								%button.btn.btn-default{:type => "button"}
									%i.fa.fa-search{"aria-hidden" => "true"}

					.col-md-4.hidden-sm.hidden-xs#metube-nav-right
						%p{:class => ['navbar-text', 'navbar-right','navbar-metube-wrapper'],}
							%a{:class => ['navbar-link', 'navbar-metube-text'],:href => "#"}
								%i.fa.fa-upload{"aria-hidden" => "true"}
								%span	Upload
							%a{:class => ['navbar-link', 'navbar-metube-text'],:href => "#"}
								%i.fa.fa-user-circle-o{"aria-hidden" => "true"}
								%span Login/Register

